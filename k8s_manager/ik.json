{
    "mappings": {
        "_default_": {
            "dynamic_templates": [
                {
                    "message_field": {
                        "mapping": {
                            "norms": false,
                            "type": "text"
                        },
                        "match_mapping_type": "string",
                        "path_match": "message"
                    }
                },
                {
                    "string_fields": {
                        "mapping": {
                            "analyzer": "by_max_word",
                            "fields": {
                                "keyword": {
                                    "type": "keyword"
                                }
                            },
                            "norms": false,
                            "type": "text"
                        },
                        "match": "*",
                        "match_mapping_type": "string"
                    }
                }
            ],
            "properties": {
                "@timestamp": {
                    "type": "date"
                },
                "@version": {
                    "type": "keyword"
                }
            }
        }
    },
    "settings": {
        "index": {
            "analysis": {
                "analyzer": {
                    "by_max_word": {
                        "char_filter": [
                            "by_cfr"
                        ],
                        "filter": [
                            "by_tfr",
                            "by_sfr"
                        ],
                        "tokenizer": "ik_max_word",
                        "type": "custom"
                    },
                    "by_smart": {
                        "char_filter": [
                            "by_cfr"
                        ],
                        "filter": [
                            "by_tfr",
                            "by_sfr"
                        ],
                        "tokenizer": "ik_smart",
                        "type": "custom"
                    }
                },
                "char_filter": {
                    "by_cfr": {
                        "mappings": [
                            "| => |"
                        ],
                        "type": "mapping"
                    }
                },
                "filter": {
                    "by_sfr": {
                        "synonyms_path": "analysis/synonyms.txt",
                        "type": "synonym"
                    },
                    "by_tfr": {
                        "stopwords": [
                            " "
                        ],
                        "type": "stop"
                    }
                }
            }
        },
        "index.refresh_interval": "5s"
    },
    "template": "*",
    "version": 50001
}